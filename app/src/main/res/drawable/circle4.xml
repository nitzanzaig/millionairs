    <item>
        <shape
            android:shape="ring"
            android:thicknessRatio="16"
            android:useLevel="false">
            <solid android:color="#DDD" />
        </shape>
    </item>
    <item>
        <shape
            android:shape="ring"
            android:thicknessRatio="16"
            android:useLevel="true">
            <solid android:color="#D57D1AB8" />
        </shape>

        <rotate
            android:fromDegrees="270"
            android:toDegrees="270">

        </rotate>
    </item>
</layer-list>